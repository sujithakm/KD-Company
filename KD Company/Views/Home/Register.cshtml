
@{
    ViewData["Title"] = "Register";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="col-md-5">
    <div class="right-content">
        <div class="top-content">
            <h6>Register your free account and <em>get immediate</em></h6>
        </div>
        <form id="contact" action="" method="get">
            <div class="row">
                <div class="col-md-12">
                    <fieldset>
                        <input name="name" type="text" class="form-control" id="name" placeholder="Your Name" required="">
                    </fieldset>
                </div>
                <div class="col-md-12">
                    <fieldset>
                        <input name="Place" type="text" class="form-control" id="place" placeholder="Your Place" required="">
                    </fieldset>
                </div>
                <div class="col-md-12">
                    <fieldset>
                        <input name="city" type="text" class="form-control" id="city" placeholder="Your City" required="">
                    </fieldset>
                </div>
                <div class="col-md-12">
                    <fieldset>
                        <input name="pincode" type="text" class="form-control" id="pincode" placeholder="Your Pincode" required="">
                    </fieldset>
                </div>
                <div class="col-md-12">
                    <fieldset>
                        <input name="phone-number" type="text" class="form-control" id="phone-number" placeholder="Your Phone Number" required="">
                    </fieldset>
                </div>
                <div class="col-md-12">
                    <fieldset>
                        <input name="email" type="text" class="form-control" id="email" placeholder="Your Email" required="">
                    </fieldset>
                </div>
                <div class="col-md-12">
                    <fieldset>
                        <input name="password" type="text" class="form-control" id="password" placeholder="Your Password" required="">
                    </fieldset>
                </div>

                <div class="col-md-12">
                    <fieldset>
                        <button type="submit" id="form-submit" class="button">Register</button>
                    </fieldset>
                </div>
            </div>
        </form>
    </div>
</div>



