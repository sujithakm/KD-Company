
@{
    ViewData["Title"] = "Register";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="section coming-soon" data-section="section3">
    <div class="container">
        <div class="row">
            <div class="col-md-6">
                <div class="right-content">
                    <div class="top-content">
                        <h6>Register your free account</h6>
                    </div>
                    <form id="contact" asp-controller="Home" asp-action="Registration" method="post">
                        <div class="row">
                            <div class="col-md-12">
                                <fieldset>
                                    <input name="Name" type="text" class="form-control" id="name" placeholder="Your Name" required="">
                                </fieldset>
                            </div>
                            <div class="col-md-12">
                                <fieldset>
                                    <input name="Place" type="text" class="form-control" id="place" placeholder="Your Place" required="">
                                </fieldset>
                            </div>
                            <div class="col-md-12">
                                <fieldset>
                                    <input name="City" type="text" class="form-control" id="city" placeholder="Your City" required="">
                                </fieldset>
                            </div>
                            <div class="col-md-12">
                                <fieldset>
                                    <input name="Pincode" type="text" class="form-control" id="pincode" placeholder="Your Pincode" required="">
                                </fieldset>
                            </div>
                            <div class="col-md-12">
                                <fieldset>
                                    <input name="Phonenumber" type="text" class="form-control" id="Phonenumber" placeholder="Your Phonenumber" required="">
                                </fieldset>
                            </div>
                            <div class="col-md-12">
                                <fieldset>
                                    <input name="Email" type="text" class="form-control" id="email" placeholder="Your Email" required="">
                                </fieldset>
                            </div>
                            <div class="col-md-12">
                                <fieldset>
                                    <button type="submit" id="form-submit" class="button">Register</button>
                                </fieldset>
                            </div>
                        </div>
                    </form>
                </div>
            </div>

        </div>
    </div>
</section>

